print("Welcome to the secret auction program.\n")

auction_members = {}

name = input("What's your name?\n")
bid = int(input("What's your bid?\n$")
other_members = input("Are there any other bidders? Type yes or no.\n"

def auction_registration(n, b):
    new_entry = {}
    new_entry[name] = n
    new_entry[bid] = b
    auction_members.append(new_entry);
    other_members = input("Are there any other bidders? Type yes or no.\n"

while(other_members == "yes"):
    auction_registration()

    
print("The winner of the bid is: ", )   

